{
	"manifest_version": 3,
	"version": "1",
	"name": "Library Search",
	"author": "https://github.com/suffs811", 
	"permissions": [
	"activeTab",
	 "tabs"
	],
	"action": {
	"default_title": "Lib Search",
	"default_popup": "popup.html"
	},
	"content_scripts": [
  	{
		"matches": ["*://*/*"],
		"css": ["style.css"]
	}
	]
}
